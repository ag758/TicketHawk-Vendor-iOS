# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'TicketHawk' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # pods for Stripe
  pod 'Alamofire'
  pod 'Stripe'

  # Pods for Firebase
  pod ‘Firebase/Core’
  pod ‘Firebase/Auth’
  pod ‘Firebase/Database’
  pod 'Firebase/Messaging'
  pod 'FirebaseUI'

  # Pods for Facebook
  pod ‘Bolts’
  pod ‘FBSDKCoreKit’
  pod ‘FBSDKLoginKit’

  pod 'IQKeyboardManagerSwift'

  target 'TicketHawkTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'TicketHawkUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
